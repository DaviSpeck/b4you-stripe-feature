{
  "name": "mango5-api",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "clean:workspace": "rm -rf node_modules/ && rm -rf yarn.lock && rm -rf package-lock.json && yarn",
    "debug": "env NODE_ENV=dev nodemon --inspect server.js",
    "dev": "env NODE_ENV=dev nodemon -L --watch . --ext js,json,yaml --ignore node_modules --ignore .git --ignore logs server.js",
    "lint-staged": "lint-staged",
    "migrate:create": "yarn sequelize migration:create --name",
    "migrate:dev:undo": "env NODE_ENV=dev yarn sequelize db:migrate:undo",
    "migrate:dev": "env NODE_ENV=dev yarn sequelize db:migrate",
    "prod": "env NODE_ENV=prod node server.js",
    "seed:dev": "env NODE_ENV=dev yarn sequelize db:seed:all",
    "start": "env NODE_ENV=dev nodemon server.js",
    "test": "env NODE_ENV=dev jest --passWithNoTests",
    "test:watch": "npm test -- --watch --detectOpenHandles",
    "test:verbose": "npm test -- --silent=false",
    "test:coverage": "npm test -- --coverage",
    "test:emails:producer": "env NODE_ENV=dev node ./tests/emails/producer/index.js",
    "test:emails:student": "env NODE_ENV=dev node ./tests/emails/student/index.js",
    "test:emails:support": "env NODE_ENV=dev node ./tests/emails/support/index.js",
    "build:docker:sandbox": "cp .env.sandbox .env.dev && docker build -t sixbase-sandbox . && docker tag sixbase-sandbox:latest 493067706051.dkr.ecr.sa-east-1.amazonaws.com/sixbase-sandbox:latest && docker push 493067706051.dkr.ecr.sa-east-1.amazonaws.com/sixbase-sandbox:latest",
    "build:docker:production": "cp .env.production .env.dev && docker build -t sixbase . && docker tag sixbase:latest 493067706051.dkr.ecr.sa-east-1.amazonaws.com/sixbase:latest && docker push 493067706051.dkr.ecr.sa-east-1.amazonaws.com/sixbase:latest",
    "swagger:paths": "node scripts/generatePaths.js",
    "swagger:schemas": "node scripts/generateSchemas.js",
    "swagger:bundle": "node scripts/generateSwaggerComplete.js",
    "swagger:generate": "yarn swagger:paths && yarn swagger:schemas && yarn swagger:bundle"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run test"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  },
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.319.0",
    "@aws-sdk/s3-request-presigner": "^3.319.0",
    "@cnpja/sdk": "^1.7.2",
    "@fast-csv/format": "^5.0.2",
    "aws-sdk": "^2.1354.0",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "consultar-cnpj": "^1.0.19",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "creditcard.js": "^3.0.30",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "geoip-lite": "^1.4.10",
    "he": "^1.2.0",
    "helmet": "^6.1.4",
    "intl": "^1.2.5",
    "ip": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "nanoid": "3.3.4",
    "node-mailjet": "^5.1.1",
    "pdf-lib": "^1.17.1",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^4.6.5",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.31.0",
    "sharp": "^0.32.0",
    "shopify-api-node": "^3.13.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "xml-js": "^1.6.11",
    "yamljs": "^0.3.0",
    "youtube-node": "^1.3.3",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@types/jest": "^29.5.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "git-commit-msg-linter": "^5.0.4",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3"
  }
}
